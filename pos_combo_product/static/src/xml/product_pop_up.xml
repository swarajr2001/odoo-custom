<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="PosComboPopUp" owl="1">
            <Draggable>
                <div class="popup">
                     <p class="title">
                        <t t-esc="props.title" />
                    </p>

                    <footer class="footer">
                        <div class="button confirm highlight" t-on-mousedown.prevent="confirm">
                            <span>Exit</span>
                        </div>
                    </footer>
                </div>
            </Draggable>
    </t>
</templates>

